{
    "manifest_version": 2,
    "name": "AtCoder ACer",
    "description": "問題一覧ページにACした人数を表示します",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://beta.atcoder.jp/contests/*/tasks",
                "https://*.contest.atcoder.jp/assignments"
            ],
            "js": ["js/jquery-3.3.1.min.js", "js/main.js"]
        }
    ],
    "permissions": [
        "http://beta.atcoder.jp/"
    ]
}
